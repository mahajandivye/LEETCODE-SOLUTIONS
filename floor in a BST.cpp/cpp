class Solution{

public:
    int floor(Node* root, int x) {
        // Code here
        int ans = -1;
        while(root)
        {
            if(root->data > x) root = root->left;
            else if(root->data < x){ans = root->data; root = root->right;}
            else if(root->data == x)return x;
        }
        return ans;
    }
};